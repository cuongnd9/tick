{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "ES2015",
      "ES2016",
      "ES2017",
      "ES2018"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
